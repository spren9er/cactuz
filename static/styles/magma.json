{
  "node": {
    "strokeColor": "#333333"
  },
  "edge": {
    "strokeColor": "#ffffff"
  },
  "link": {
    "strokeColor": "#333333"
  },
  "label": {
    "inner": {
      "textColor": "#efefef",
      "fontFamily": "monospace",
      "minFontSize": 9,
      "maxFontSize": 14
    },
    "outer": {
      "textColor": "#333333",
      "textOpacity": 1,
      "fontFamily": "monospace",
      "fontSize": 9,
      "link": {
        "strokeColor": "#cccccc"
      }
    }
  },
  "highlight": {
    "edgeNode": {
      "fillColor": "#ffffff",
      "strokeColor": "#cccccc"
    }
  },
  "depths": [
    {
      "depth": -1,
      "label": {
        "inner": {
          "textColor": "#333333"
        }
      },
      "highlight": {
        "node": {
          "fillColor": "#ffffff",
          "strokeColor": "#333333",
          "strokeWidth": 2
        },
        "label": {
          "inner": {
            "textColor": "#333333"
          }
        }
      }
    },
    {
      "depth": "*",
       "node": {
         "fillColor": { "scale": "magma" }
       }
     }
  ]
}
