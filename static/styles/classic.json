{
  "node": {
    "fillColor": "#efefef",
    "fillOpacity": 1,
    "strokeColor": "#aaaaaa",
    "strokeOpacity": 1,
    "strokeWidth": 0
  },
  "edge": {
    "strokeColor": "#efefef",
    "strokeOpacity": 0.2,
    "strokeWidth": 1
