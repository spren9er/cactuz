        }
      },
      "highlight": {
        "node": {
          "fillColor": "#ffbbb7",
          "fillOpacity": 1,
          "strokeColor": "#ea575a",
          "strokeOpacity": 1,
          "strokeWidth": 2
        },
        "label": {
          "inner": {
            "textColor": "#ea575a"
