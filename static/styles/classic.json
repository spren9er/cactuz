      }
    }
  },
  "link": {
    "strokeColor": "#aaaaaa",
    "strokeOpacity": 1,
    "strokeWidth": 1
  },
  "highlight": {
    "node": {
      "fillOpacity": 0.8,
      "strokeColor": "#333333",
      "strokeOpacity": 1,
      "strokeWidth": 1
    },
    "edge": {
      "strokeColor": "#ea575a",
      "strokeOpacity": 1,
      "strokeWidth": 1
