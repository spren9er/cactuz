    "fillOpacity": 1,
    "strokeOpacity": 1
  },
  "label": {
    "inner": {
      "textColor": "#efefef",
      "textOpacity": 1,
      "fontFamily": "monospace",
      "fontWeight": "normal",
      "minFontSize": 9,
      "maxFontSize": 14
    },
    "outer": {
      "textColor": "#333333",
      "textOpacity": 1,
      "fontFamily": "monospace",
      "fontWeight": "normal",
      "fontSize": 9,
      "padding": 1,
      "link": {
        "strokeColor": "#cccccc",
        "strokeOpacity": 1,
        "strokeWidth": 1,
        "padding": 0,
